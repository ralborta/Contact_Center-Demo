# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/contact_center?schema=public

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# App
APP_PORT=3000

# ElevenLabs
ELEVENLABS_WEBHOOK_TOKEN=your_elevenlabs_webhook_token_here
ELEVENLABS_PROVIDER_NAME=elevenlabs

# builderbot.cloud
BUILDERBOT_WEBHOOK_TOKEN=your_builderbot_webhook_token_here
BUILDERBOT_API_URL=https://api.builderbot.cloud
BUILDERBOT_API_KEY=your_builderbot_api_key_here

# Twilio SMS
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_FROM_NUMBER=+1234567890
TWILIO_WEBHOOK_TOKEN=your_twilio_webhook_token_here

# OTP
OTP_TTL_SECONDS=300
OTP_MAX_ATTEMPTS=5
OTP_RATE_LIMIT_WINDOW_SECONDS=900
OTP_RATE_LIMIT_MAX=3

# PII
PII_MASKING_ENABLED=true
